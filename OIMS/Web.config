  <system.web>
    <!--To fix HTTP Error 404.15-->
    <httpRuntime targetFramework="4.5" maxQueryStringLength="32768" maxUrlLength="65536"/>
    <compilation targetFramework="4.5" debug="true">
      <assemblies>
        <add assembly="DevExpress.Data.v13.2"/>

        <add assembly="DevExpress.Web.ASPxHtmlEditor.v13.2"/>
        <add assembly="DevExpress.Web.ASPxSpellChecker.v13.2"/>
        <add assembly="DevExpress.Web.ASPxTreeList.v13.2"/>
